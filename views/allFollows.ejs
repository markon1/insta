<!DOCTYPE html>
<html>

<head>
    <title>All Follows</title>
    <style type="text/css">
        table {
            border-collapse: collapse;
            text-align: center;
        }

        table,
        th,
        td {
            border: 1px solid black;
        }
    </style>
</head>

<body>
    <a href='/csv'>Get CSV</a>
    <br>
    <table>
        <tr>
            <th>Brand</th>
            <th>Instagram Profile</th>
        </tr>
        <% follows.forEach(function(f){ %>
            <tr>
                <td>
                    <%= f.brand %>
                </td>
                <td>
                    <%= f.handle %>
                </td>
            </tr>
            <% }) %>
    </table>
</body>

</html>